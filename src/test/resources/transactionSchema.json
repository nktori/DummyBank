{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Transaction",
  "type": "object",
  "properties": {
    "payeeId": {
      "type": "integer"
    },
    "payerId": {
      "type": "integer"
    },
    "amount": {
      "type": "integer",
      "minimum": 1
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "payeeId",
    "payerId",
    "amount",
    "timestamp"
  ]
}
